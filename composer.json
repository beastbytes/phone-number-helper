{
    "name": "beastbytes/phone-number-helper",
    "type": "library",
    "description": "Helper methods to format national phone numbers according to national numbering plans and convert the format of international phone numbers from ITU E.123 to EPP format",
    "keywords": [
        "phone number",
        "epp",
        "itu",
        "itu-t",
        "E.123",
        "E.164"
    ],
    "license": "BSD-3-Clause",
    "authors": [{
        "name": "Chris Yates",
        "email": "chris.l.yates@gmail.com"
    }],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.0",
        "beastbytes/phone-number-formats": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "roave/security-advisories": "dev-latest",
        "vimeo/psalm": "^4.3"
    },
    "autoload": {
        "psr-4": {
            "BeastBytes\\PhoneNumber\\Helper\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    }
}
